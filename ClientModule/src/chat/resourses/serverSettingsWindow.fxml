<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chat.client.ServerSettingsController">
   <children>
      <Label layoutX="530.0" layoutY="28.0" prefHeight="96.0" prefWidth="256.0" text="LocalChat" textFill="#790dab">
         <font>
            <Font name="Jokerman" size="44.0" />
         </font>
      </Label>
      <TextField fx:id="hostField" layoutX="259.0" layoutY="222.0" prefHeight="21.0" prefWidth="283.0" promptText="Host">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <Label fx:id="labelForFails" layoutX="259.0" layoutY="299.0" prefHeight="3.0" prefWidth="283.0" textFill="#bf1818">
         <font>
            <Font size="13.0" />
         </font></Label>
      <Label layoutX="231.0" layoutY="153.0" text="Choose server settings">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <TextField fx:id="portField" layoutX="259.0" layoutY="262.0" prefHeight="4.0" prefWidth="283.0" promptText="Port">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <Button fx:id="backButton" layoutX="259.0" layoutY="326.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="124.0" style="-fx-background-color: #9d33b8;" text="Back" textFill="#f8f8f8">
         <font>
            <Font name="Segoe UI" size="15.0" />
         </font>
      </Button>
      <Button fx:id="goToChatButton" layoutX="418.0" layoutY="326.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="124.0" style="-fx-background-color: #9d33b8;" text="Go to chat" textFill="#f8f8f8">
         <font>
            <Font name="Segoe UI" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
